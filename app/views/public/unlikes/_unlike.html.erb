<% if post.unliked_by?(current_member) %>
  <p>
    <%= link_to board_post_unlikes_path(post.board, post), method: :delete, remote: true, style:"text-decoration:none;" do %>
      <i class="fas fa-poo unlike"></i><span class="unlike"><%= post.unlikes.count %></span>
    <% end %>
  </p>
<% else %>
  <p>
    <%= link_to board_post_unlikes_path(post.board, post), method: :post, remote: true, style:"text-decoration:none;" do %>
      <i class="fas fa-poo reverse_unlike"></i><span class="reverse_unlike"><%= post.unlikes.count %></span>
    <% end %>
  </p>
<% end %>