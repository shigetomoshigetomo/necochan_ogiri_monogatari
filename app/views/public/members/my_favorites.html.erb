<div class="text-center mb-3">
  <%= image_tag "favorite.png" %>
</div>

<% @favorites_post.each do |post| %>
  <div class="box mb-2">
    <%= render "public/boards/show", board: post.board %>
    <%= render "public/posts/show", board: post.board, post: post %>
    <div class="d-flex mt-2">
      <%= render "public/favorites/favorite", obj: post.board, post: post %>
      <%= render "public/unlikes/unlike", obj: post.board, post: post %>
    </div>
  </div>
<% end %>